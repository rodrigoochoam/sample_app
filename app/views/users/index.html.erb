<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate @users, 
    class: "pagination justify-content-center",
    inner_window: 2,
    outer_window: 1
%>

<ul class="users">
  <% @users.each do |user| %>
    <%= render user %>
  <% end %>
</ul>

<%= will_paginate @users, 
    class: "pagination justify-content-center",
    inner_window: 2,
    outer_window: 1
%>
